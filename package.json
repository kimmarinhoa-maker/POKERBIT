{
  "name": "poker-manager-saas",
  "version": "0.1.0",
  "description": "Poker Manager - migracao SaaS (Fase 0: golden tests)",
  "private": true,
  "scripts": {
    "dev": "npx concurrently -n api,web -c blue,green \"npm run dev --prefix apps/api\" \"npm run dev --prefix apps/web\"",
    "dev:api": "npm run dev --prefix apps/api",
    "dev:web": "npm run dev --prefix apps/web",
    "golden:run": "node tools/run_golden.js",
    "golden:compare": "node tools/compare.js",
    "golden:update": "node tools/run_golden.js --update",
    "golden:inspect": "node tools/run_golden.js --inspect",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:golden": "node tools/compare.js"
  },
  "dependencies": {
    "pg": "^8.18.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "vitest": "^4.0.18"
  }
}
